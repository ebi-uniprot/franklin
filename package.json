{
  "name": "franklin-sites",
  "description": "React and Zurb Foundation based design system for life sciences web applications",
  "version": "0.0.74",
  "main": "./dist/franklin-components.js",
  "files": [
    "src",
    "dist",
    "assets"
  ],
  "repository": "https://github.com/ebi-uniprot/franklin.git",
  "author": "Xavier Watkins <xwatkins@ebi.ac.uk>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build-library && yarn build-storybook",
    "build-library": "webpack --config webpack.library.js",
    "test:lint": "eslint -c .eslintrc.json --ext .js --ext .jsx src",
    "test:unit": "jest",
    "test": "yarn test:lint && yarn test:unit",
    "test-watch": "jest --watch",
    "release": "yarn build && yarn publish && git push --follow-tags origin master",
    "start": "start-storybook -p 6006 -s ./assets",
    "build-storybook": "build-storybook -c .storybook -o .out -s ./assets"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "IE 11"
  ],
  "jest": {
    "transform": {
      "^.+\\.(jsx|js)?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "/src/testHelpers/expectToThrowNoConsole.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|sass)$": "<rootDir>/__mocks__/styleMock.js",
      "^lodash-es$": "lodash"
    }
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "d3": "5.16.0",
    "foundation-sites": "^6.5.3",
    "history": "^4.10.1",
    "lodash-es": "^4.17.15",
    "lorem-ipsum": "^2.0.2",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "query-string": "^6.13.1",
    "react": "^16.9.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.9.0",
    "react-markdown": "^4.2.2",
    "react-router-dom": "^5.0.1",
    "react-scrollspy": "^3.4.3",
    "react-sizeme": "^2.6.12",
    "rheostat": "^4.1.1",
    "timing-functions": "^2.0.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/api": "^6.0.21",
    "@storybook/components": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@svgr/webpack": "^5.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.0.4",
    "browser-sync-webpack-plugin": "^2.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.2",
    "eslint": "7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "6.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.4.2",
    "mini-css-extract-plugin": "^0.11.0",
    "mkdirp": "^1.0.4",
    "node-sass": "^4.9.1",
    "prettier": "^2.1.1",
    "random": "^2.2.0",
    "react-docgen": "^5.3.0",
    "react-test-renderer": "^16.9.0",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "^10.0.2",
    "storybook-react-router": "^1.0.8",
    "style-loader": "^1.2.1",
    "svg-url-loader": "^6.0.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.1.2"
  }
}
